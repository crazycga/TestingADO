	},
	"links": {
		"home": {
			"uri": "http://evergrowth.ca"
		},
		"repository": {
			"uri": "https://github.com/crazycga/TestingADO"
		},
		"issues": {
			"uri": "https://github.com/crazycga/TestingADO/issues"
		},
		"support": {
			"uri": "https://github.com/crazycga/TestingADO/issues"
		},
		"privacypolicy": {
